<p id="notice"><%= notice %></p>

<h1>Listing G Posts</h1>

<table>
  <thead>
    <tr>
      <th>Lat</th>
      <th>Lon</th>
      <th>Image</th>
      <th>User</th>
      <th>User name</th>
      <th>Park</th>
      <th>Ip address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @g_posts.each do |g_post| %>
      <tr>
        <td><%= g_post.lat %></td>
        <td><%= g_post.lon %></td>
        <td><%= g_post.image %></td>
        <td><%= g_post.user_id %></td>
        <td><%= g_post.user_name %></td>
        <td><%= g_post.park %></td>
        <td><%= g_post.ip_address %></td>
        <td><%= link_to 'Show', g_post %></td>
        <td><%= link_to 'Edit', edit_g_post_path(g_post) %></td>
        <td><%= link_to 'Destroy', g_post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New G post', new_g_post_path %>
